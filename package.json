{
  "name": "reportedegastos.org",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "command": "nestjs-command",
    "dev": "npm run docker:dev:up --build",
    "docker:dev:down": "docker-compose -f docker/development/docker-compose.yml down",
    "docker:dev:logs": "docker logs -f api-backend",
    "docker:dev:restart": "npm run docker:dev:down && npm run docker:dev:up",
    "docker:dev:up": "docker-compose -f docker/development/docker-compose.yml up",
    "docker:staging:down": "docker-compose -f docker/staging/docker-compose.yml down",
    "docker:staging:restart": "npm run docker:staging:down && npm run docker:staging:up",
    "docker:staging:up": "docker-compose -f docker/staging/docker-compose.yml up",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prebuild": "rimraf dist",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "start": "nest start",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test": "jest"
  },
  "dependencies": {
    "@meanie/mongoose-to-json": "^2.5.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "aws-sdk": "^2.889.0",
    "bcrypt": "^5.0.1",
    "casbin-mongodb-adapter": "^1.2.0",
    "casbin": "^3.0.3",
    "casbin-mongoose-adapter": "^1.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "deep-populate": "^1.0.0",
    "joi": "^17.4.0",
    "mongodb": "^3.6.6",
    "mongoose": "^5.12.5",
    "mongoose-beautiful-unique-validation": "^7.1.1",
    "mongoose-delete": "^0.5.3",
    "mongoose-paginate": "^5.0.3",
    "mongoose-query-parser": "^1.1.9",
    "mongoose-unique-validator": "^2.0.3",
    "nestjs-command": "^2.0.0",
    "nestjs-s3": "^1.0.1",
    "nestjs-seeder": "^0.2.0",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "ramda": "^0.27.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "uuid-apikey": "^1.5.2",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.5.3",
    "@types/jest": "^26.0.15",
    "@types/mongoose-paginate": "^5.0.8",
    "@types/node": "^14.14.6",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "@types/uuid-apikey": "^1.4.0",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint": "^7.12.1",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "webpack": "^5.35.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@app/mongoose-uuid/(.*)": "<rootDir>/libs/mongoose-uuid/src/$1",
      "@app/mongoose-uuid": "<rootDir>/libs/mongoose-uuid/src",
      "@app/casbin/(.*)": "<rootDir>/libs/casbin/src/$1",
      "@app/casbin": "<rootDir>/libs/casbin/src"
    }
  }
}
